[global]
port = 1701
auth file = /etc/xl2tpd/xl2tp-secrets
access control = no

[lns default]
exclusive = yes
ip range = 192.168.254.202-192.168.254.210
lac = 10.0.1.2
hidden bit = no
local ip = 192.168.254.200
length bit = yes
refuse authentication = yes
name = VersaLink
ppp debug = yes
pppoptfile = /etc/ppp/options.xl2tpd

[lac bras]
lns = bras.seu.edu.cn
refuse authentication = no
require chap = yes
name = |USERNAME|
ppp debug = yes
pppoptfile = /etc/ppp/options.bras.xl2tpd
